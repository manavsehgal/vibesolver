{
  "name": "vibesolver",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "preview": "vite preview",
    "serve:prod": "node server.js",
    "start:prod": "pnpm build && pnpm serve:prod",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "metrics": "node scripts/collect-metrics.js",
    "metrics:summary": "node scripts/metrics-summary.js",
    "metrics:compare": "node scripts/compare-metrics.js",
    "metrics:watch": "node scripts/collect-metrics.js && echo 'Report generated! Check reports/ folder'"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "ai": "^4.3.16",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "drizzle-orm": "^0.44.0",
    "drizzle-zod": "^0.8.2",
    "postcss": "^8.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.25.41",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.27",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.1.4",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.18.0",
    "helmet": "^8.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.4",
    "workbox-window": "^7.3.0"
  }
}
